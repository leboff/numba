<div id="<%= dom_id(listing) %>" class="listing">
  <h2> <%= listing.name %>  </h2>
  <% Rails.logger.info @connection.to_yaml %>
  <%= link_to '', @connection ? @connection : listing, :confirm => 'Are you sure?', :method => :delete, :remote => true, :class => "delete_listing" %>
  <ul class="listing_list">
    <%= render listing.listing_items %>
  </ul>
</div>
